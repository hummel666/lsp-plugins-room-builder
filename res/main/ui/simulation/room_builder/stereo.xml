<plugin resizable="true">
	<vbox spacing="4">
		<!-- Main settings -->
		<hbox spacing="4">
			<label text="labels.fft.frame" />
			<combo id="fft" />
			<vsep />
			<button id="wpp" size="16" color="green" led="true" />
			<label text="labels.ir_equalizer" />
			<void hexpand="true" />
		</hbox>
		
		<hbox spacing="4" expand="true">
			<!-- IR editor -->
			<cgroup id="view" expand="true">
				<vbox spacing="2">
					<hbox spacing="4">
						<vbox vfill="false">
							<align hpos="-1"><label text="labels.room_orientation:" /></align>
							<align hpos="-1"><combo id="ifo" /></align>
						</vbox>
						<vsep />
						
						<knob id="xscale" scolor="red" size="16" />
						<vbox vfill="false">
							<align hpos="-1"><label text="labels.3d.s_ize_x:" /></align>
							<align hpos="-1"><value id="xscale" sline="true" /></align>
						</vbox>
						
						<knob id="yscale" scolor="green"  size="16" />
						<vbox vfill="false">
							<align hpos="-1"><label text="labels.3d.s_ize_y:" /></align>
							<align hpos="-1"><value id="yscale" sline="true" /></align>
						</vbox>
						
						<knob id="zscale" scolor="blue" size="16" />
						<vbox vfill="false">
							<align hpos="-1"><label text="labels.3d.s_ize_z:" /></align>
							<align hpos="-1"><value id="zscale" sline="true" /></align>
						</vbox>
						<void hexpand="true" />
					</hbox>
				
					<area3d expand="true"
						id="ifn"
						width.min="340" height.min="210"
						pos.x.id="cposx" pos.y.id="cposy" pos.z.id="cposz"
						scale.x.id="xscale" scale.y.id="yscale" scale.z.id="zscale"
						yaw.id="cyaw" pitch.id="cpitch"
						fov="70"
						orientation_id="ifo" status_id="ifs"
						kvt_root="/scene/object/">
						
						<origin3d />
						
						<ui:for id="i" first="0" last="7">
							<source3d
								visibility=":sse_${i}" 
								mode_id="sscf_${i}"
								hue_id="ssh_${i}" hue_shift="0.1"
								xpos_id="sspx_${i}" ypos_id="sspy_${i}" zpos_id="sspz_${i}"
								yaw_id="ssay_${i}" pitch_id="ssap_${i}" roll_id="ssar_${i}"
								size_id="sss_${i}" angle_id="ssa_${i}" height_id="shh_${i}" curvature_id="sscv_${i}"
							/>
						</ui:for>
						
						<ui:for id="i" first="0" last="7" >
							<capture3d
								visibility=":sce_${i}" 
								mode_id="sccf_${i}"
								hue_id="sch_${i}" hue_shift="0.1"
								xpos_id="scpx_${i}" ypos_id="scpy_${i}" zpos_id="scpz_${i}"
								yaw_id="scay_${i}" pitch_id="scap_${i}" roll_id="scar_${i}"
								size_id="sccs_${i}" angle_id="sca_${i}" distance_id="scab_${i}"
							/>
						</ui:for>
					</area3d>
					
					<grid rows="4" cols="7">
						<label text="labels.3d.3d_model" />
						<cell rows="4"><vsep /></cell>
						<cell cols="5"><label text="labels.3d.camera" /></cell>
						
						<cell rows="3">
							<load id="ifn" progress.id="ifp"
								status=":ifs" fill="false"
								format="obj3d" path.id="_ui_dlg_model3d_path" />
						</cell>
						
						<label text="labels.3d.x_axis" />
						<label text="labels.3d.y_axis" />
						<label text="labels.3d.z_axis" />
						<label text="labels.3d.yaw" />
						<label text="labels.3d.pitch" />
						
						<knob id="cposx" size="24" scolor="red" />
						<knob id="cposy" size="24" scolor="green" />
						<knob id="cposz" size="24" scolor="blue" />
						<knob id="cyaw" size="24" />
						<knob id="cpitch" size="24" />
						
						<value id="cposx" sline="true" />
						<value id="cposy" sline="true" />
						<value id="cposz" sline="true" />
						<value id="cyaw" sline="true" />
						<value id="cpitch" sline="true" />
					</grid>
				</vbox>
				
				<!-- Sample editors -->
				<ui:for id="i" first="0" last="7">
					<vbox spacing="2">
						<hbox border="2">
							<sample
								head_id="ihc_${i}" tail_id="itc_${i}"
								fadein_id="ifi_${i}" fadeout_id="ifo_${i}" length_id="ifl_${i}"
								duration_id="sdur_${i}" max_duration_id="smdur_${i}"
								status_id="ifs_${i}" mesh_id="ifd_${i}"
								width="340" height="210"
							/>
						</hbox>
						
						<grid rows="4" cols="8" hspacing="4">
							<label text="labels.export" />
							<cell rows="4"><vsep /></cell>
							<label text="labels.sedit.reverse" />
							<label text="labels.sedit.head_cut" />
							<label text="labels.sedit.tail_cut" />
							<label text="labels.sedit.fade_in" />
							<label text="labels.sedit.fade_out" />
							<label text="labels.sedit.listen" />
							
							<cell rows="3">
								<save id="ofn_${i}" command.id="ofc_${i}" progress.id="ofp_${i}"
									status=":ofs_${i}" fill="false"
									format="wav,lspc,all" path.id="_ui_dlg_sample_path" 
								/>
							</cell>
							<button id="irv_${i}" size="32" toggle="false" />
							<knob id="ihc_${i}" size="20" />
							<knob id="itc_${i}" size="20" />
							<knob id="ifi_${i}" size="20" scolor="yellow" />
							<knob id="ifo_${i}" size="20" scolor="yellow" />
							<button id="ils_${i}" size="32" toggle="false" />

							<led id="irv_${i}" />
							<value id="ihc_${i}" />
							<value id="itc_${i}" />
							<value id="ifi_${i}" />
							<value id="ifo_${i}" />
							<led id="ils_${i}" />
							
							<label text="labels.sedit.makeup" />
							<cell cols="3">
								<fader id="imkp_${i}" size="20" />
							</cell>
							<cell cols="2">
								<value id="imkp_${i}" sline="true" />
							</cell>
						</grid>
					</vbox>
				</ui:for>
			</cgroup>
			<cgroup id="editor" valign="-1" vscale="0">
				<!-- Source editor -->
				<vbox spacing="2">
					<!-- Source selector -->
					<hbox spacing="8">
						<label text="labels.source:" />
						<combo id="ssel" width="20" />
						<button id="sse[ssel]" size="16" led="true" color="red" hue_id="ssh[ssel]" />
						<label text="labels.src.enabled" />
						<void hexpand="true"/>
					</hbox>
					<hsep pad_bottom="8" />
					
					<!-- Sources -->
					<grid rows="2" cols="2" transpose="true" vspacing="2" hspacing="4" fill="true">
						<align pad_left="8" hpos="-1"><label text="labels.3d.shape" /></align>
						<align pad_left="8" hpos="-1"><label text="labels.signal.phase" /></align>
					
						<combo id="sscf[ssel]" />
						<hbox spacing="2">
							<button id="ssph[ssel]" size="16" led="true" color="red" hue_id="ssh[ssel]" />
							<label text="labels.signal.invert" />
							<void hexpand="true" />
						</hbox>
					</grid>

					<grid rows="12" cols="3">
						<cell cols="3"><hsep /></cell>
						<label text="labels.3d.x_axis" />
						<label text="labels.3d.y_axis" />
						<label text="labels.3d.z_axis" />
						
						<knob id="sspx[ssel]" size="16" scolor.hue=":ssh[ssel]" />
						<knob id="sspy[ssel]" size="16" scolor.hue=":ssh[ssel]" />
						<knob id="sspz[ssel]" size="16" scolor.hue=":ssh[ssel]" />
						
						<value id="sspx[ssel]" sline="true" />
						<value id="sspy[ssel]" sline="true" />
						<value id="sspz[ssel]" sline="true" />
						
						<cell cols="3"><hsep /></cell>
						<label text="labels.3d.yaw" />
						<label text="labels.3d.pitch" />
						<label text="labels.3d.roll" />
						
						<knob id="ssay[ssel]" size="16" scolor.hue=":ssh[ssel]" />
						<knob id="ssap[ssel]" size="16" scolor.hue=":ssh[ssel]" />
						<knob id="ssar[ssel]" size="16" scolor.hue=":ssh[ssel]" />
						
						<value id="ssay[ssel]" sline="true" />
						<value id="ssap[ssel]" sline="true" />
						<value id="ssar[ssel]" sline="true" />
						
						<cell cols="3"><hsep /></cell>
						<label text="labels.obj.hue" />
						<label text="labels.3d.angle" />
						<label text="labels.3d.size" />
						
						<knob id="ssh[ssel]" size="16" scolor.hue=":ssh[ssel]" />
						<knob id="ssa[ssel]" size="16" scolor.hue=":ssh[ssel]" balance="50" />
						<knob id="sss[ssel]" size="16" scolor.hue=":ssh[ssel]" />
						
						<value id="ssh[ssel]" sline="true" />
						<value id="ssa[ssel]" sline="true"/>
						<value id="sss[ssel]" sline="true"/>
					</grid>
					
					<hbox spacing="2">
						<vbox visibility="(:sscf[ssel] ieq 5) or (:sscf[ssel] ieq 6) or (:sscf[ssel] ieq 10)">
							<label text="labels.3d.height" />
							<knob id="shh[ssel]" size="16" scolor.hue=":ssh[ssel]" />
							<value id="shh[ssel]" sline="true"/>
						</vbox>
						<vbox visibility="(:sscf[ssel] ieq 10) or (:sscf[ssel] ieq 11)">
							<label text="labels.3d.curvature" />
							<knob id="sscv[ssel]" size="16" scolor.hue=":ssh[ssel]" />
							<value id="sscv[ssel]" sline="true"/>
						</vbox>
					</hbox>
				</vbox>

				<!-- Capture editor -->
				<vbox>
					<!-- Capture selector -->
					<align hpos="-1" vpos="-1">
						<hbox spacing="8">
							<label text="labels.capture:" />
							<combo id="csel" width="20" />
							<button id="sce[csel]" size="16" led="true" color="red" hue_id="sch[csel]" />
							<label text="labels.obj.enabled" />
						</hbox>
					</align>
					<hsep pad_bottom="8" />
					
					<!-- Captures -->
					<align vpos="-1">
						<grid rows="4" cols="2" transpose="true" vspacing="2" hspacing="4" >
							<align pad_left="8" hpos="-1"><label text="labels.mode" /></align>
							<align pad_left="8" hpos="-1"><label text="labels.reflections" /></align>
							<align pad_left="8" hpos="-1"><label text="labels.mic_rophone_direction" /></align>
							<align pad_left="8" hpos="-1" visibility="(:sccf[csel] ieq 4)" ><label text="labels.side_direction" /></align>
							
							<combo id="sccf[csel]" />
							<hbox spacing="2">
								<combo id="scf[csel]" />
								<label text=" - " />
								<combo id="scl[csel]" />
							</hbox>
							<combo id="scmd[csel]" />
							<combo id="scsd[csel]" visibility="(:sccf[csel] ieq 4)" />
						</grid>
					</align>
					
					<grid rows="8" cols="3">
						<cell cols="3"><hsep /></cell>
						<label text="labels.3d.x_axis" />
						<label text="labels.3d.y_axis" />
						<label text="labels.3d.z_axis" />
						
						<knob id="scpx[csel]" size="16" scolor.hue=":sch[csel]" />
						<knob id="scpy[csel]" size="16" scolor.hue=":sch[csel]" />
						<knob id="scpz[csel]" size="16" scolor.hue=":sch[csel]" />
						
						<value id="scpx[csel]" sline="true" />
						<value id="scpy[csel]" sline="true" />
						<value id="scpz[csel]" sline="true" />
						
						<cell cols="3"><hsep /></cell>
						<label text="labels.3d.yaw" />
						<label text="labels.3d.pitch" />
						<label text="labels.3d.roll" />
						
						<knob id="scay[csel]" size="16" scolor.hue=":sch[csel]" />
						<knob id="scap[csel]" size="16" scolor.hue=":sch[csel]" />
						<knob id="scar[csel]" size="16" scolor.hue=":sch[csel]" />
						
						<value id="scay[csel]" sline="true" />
						<value id="scap[csel]" sline="true" />
						<value id="scar[csel]" sline="true" />
					</grid>
					
					<hsep />
					<hbox spacing="2">
						<vbox>
							<label text="labels.obj.hue" />
							<knob id="sch[csel]" size="16" scolor.hue=":sch[csel]" />
							<value id="sch[csel]" sline="true" />
						</vbox>
						<vbox>
							<label text="labels.capsule" />
							<knob id="sccs[csel]" size="16" scolor.hue=":sch[csel]" />
							<value id="sccs[csel]" sline="true" />
						</vbox>
						<vbox visibility="(:sccf[csel] ieq 1) or (:sccf[csel] ieq 3)" >
							<label text="labels.3d.angle" />
							<knob id="sca[csel]" size="16" scolor.hue=":sch[csel]" balance="90" />
							<value id="sca[csel]" sline="true"/>
						</vbox>
						<vbox visibility="(:sccf[csel] ieq 2)" >
							<label text="labels.3d.distance" />
							<knob id="scab[csel]" size="16" scolor.hue=":sch[csel]" />
							<value id="scab[csel]" sline="true" />
						</vbox>
					</hbox>
				</vbox>
				
				<!--  Object editor -->
				<vbox>
					<!-- Object selector -->
					<align hpos="-1" vpos="-1">
						<hbox spacing="8">
							<label text="labels.obj.object:" />
							<button id="kvt:enabled" size="16" led="true" color="red" hue_id="kvt:hue" />
							<label text="labels.obj.enabled" />
						</hbox>
					</align>
					<combo id="kvt:oid" width="192" />
					
					<grid rows="12" cols="3">
						<cell cols="3"><hsep /></cell>
						<label text="labels.3d.move_x" />
						<label text="labels.3d.move_y" />
						<label text="labels.3d.move_z" />
						
						<knob id="kvt:xpos" size="16" scale_hue="0" scolor.hue=":kvt:hue" />
						<knob id="kvt:ypos" size="16" scale_hue="0" scolor.hue=":kvt:hue" />
						<knob id="kvt:zpos" size="16" scale_hue="0" scolor.hue=":kvt:hue" />
						
						<value id="kvt:xpos" sline="true" />
						<value id="kvt:ypos" sline="true" />
						<value id="kvt:zpos" sline="true" />
						
						<cell cols="3"><hsep /></cell>
						<label text="labels.3d.yaw" />
						<label text="labels.3d.pitch" />
						<label text="labels.3d.roll" />
						
						<knob id="kvt:yaw" size="16" scale_hue="0" scolor.hue=":kvt:hue" />
						<knob id="kvt:pitch" size="16" scale_hue="0" scolor.hue=":kvt:hue" />
						<knob id="kvt:roll" size="16" scale_hue="0" scolor.hue=":kvt:hue" />
						
						<value id="kvt:yaw" sline="true" />
						<value id="kvt:pitch" sline="true" />
						<value id="kvt:roll" sline="true" />
						
						<cell cols="3"><hsep /></cell>
						<label text="labels.3d.scale_x" />
						<label text="labels.3d.scale_y" />
						<label text="labels.3d.scale_z" />
						
						<knob id="kvt:xscale" size="16" scale_hue="0" scolor.hue=":kvt:hue" />
						<knob id="kvt:yscale" size="16" scale_hue="0" scolor.hue=":kvt:hue" />
						<knob id="kvt:zscale" size="16" scale_hue="0" scolor.hue=":kvt:hue" />
						
						<value id="kvt:xscale" sline="true" />
						<value id="kvt:yscale" sline="true" />
						<value id="kvt:zscale" sline="true" />
					</grid>
					
					<hsep />
					<grid rows="3" cols="1">
						<label text="labels.obj.hue" />
						<knob id="kvt:hue" size="16" scale_hue="0" scolor.hue=":kvt:hue" />
						<value id="kvt:hue" sline="true" />
					</grid>
				</vbox>
				
				<!-- Material editor -->
				<vbox>
					<!-- Object selector -->
					<align hpos="-1" vpos="-1">
						<hbox spacing="8">
							<label text="labels.obj.object:" />
							<button id="kvt:enabled" size="16" led="true" color="red" hue_id="kvt:hue" />
							<label text="labels.obj.enabled" />
						</hbox>
					</align>
					<combo id="kvt:oid" width="192" />
					<align hpos="-1" vpos="-1">
						<label text="labels.material.material_preset:" />
					</align>
					<combo ui:id="mpreset" width="192" />
					
					<hsep />
					<hbox>
						<label text="labels.material.sound_speed" />
						<knob id="kvt:speed" size="16" scale_hue="0" scolor.hue=":kvt:hue" />
						<value id="kvt:speed" sline="true" />
					</hbox>
					<hsep />
					<align hpos="-1" vpos="-1">
						<label text="labels.material.physical_properties:" />
					</align>
					<grid rows="9" cols="4">
						<label />
						<label text="labels.material.out_to_in" />
						<label />
						<label text="labels.material.in_to_out" />
						
						<!-- Absorption -->
						<label text="labels.material.absorption" />
						<knob id="kvt:oabs" size="16" scale_hue="0" scolor.hue=":kvt:hue" />
						<cell rows="2">
							<vbox>
								<button id="kvt:labs" size="12" led="true" color="red" hue_id="kvt:hue" />
								<label font.size="9" text="labels.material.link" />
							</vbox>
						</cell>
						<knob id="kvt:iabs" size="16" scale_hue="0" scolor.hue=":kvt:hue" />
						
						<label text="labels.units.:pc" />
						<value id="kvt:oabs" detailed="false" />
						<value id="kvt:iabs" detailed="false" />
						
						<!-- Transparency -->
						<label text="labels.material.transparency" />
						<knob id="kvt:otransp" size="16" scale_hue="0" scolor.hue=":kvt:hue" />
						<cell rows="2">
							<vbox>
								<button id="kvt:ltransp" size="12" led="true" color="red" hue_id="kvt:hue" />
								<label font.size="9" text="labels.material.link" />
							</vbox>
						</cell>
						<knob id="kvt:itransp" size="16" scale_hue="0" scolor.hue=":kvt:hue" />
						
						<label text="labels.units.:pc" />
						<value id="kvt:otransp" detailed="false" />
						<value id="kvt:itransp" detailed="false" />
						
						<!-- Dispersion -->
						<label text="labels.material.dispersion" />
						<knob id="kvt:odisp" size="16" scale_hue="0" scolor.hue=":kvt:hue" />
						<cell rows="2">
							<vbox>
								<button id="kvt:ldisp" size="12" led="true" color="red" hue_id="kvt:hue" />
								<label font.size="9" text="labels.material.link" />
							</vbox>
						</cell>
						<knob id="kvt:idisp" size="16" scale_hue="0" scolor.hue=":kvt:hue" />
						
						<label />
						<value id="kvt:odisp" detailed="false" />
						<value id="kvt:idisp" detailed="false" />
						
						<!-- Diffusion -->
						<label text="labels.material.diffusion" />
						<knob id="kvt:odiff" size="16" scale_hue="0" scolor.hue=":kvt:hue" />
						<cell rows="2">
							<vbox>
								<button id="kvt:ldiff" size="12" led="true" color="red" hue_id="kvt:hue" />
								<label font.size="9" text="labels.material.link" />
							</vbox>
						</cell>
						<knob id="kvt:idiff" size="16" scale_hue="0" scolor.hue=":kvt:hue" />
						
						<label />
						<value id="kvt:odiff" detailed="false" />
						<value id="kvt:idiff" detailed="false" />
					</grid>
				</vbox>
			</cgroup>
		</hbox>
		
		<hbox spacing="4">
			<!-- Rendering -->
			<group text="groups.rendering">
				<grid rows="4" cols="3" transpose="true" vspacing="4" hspacing="4">
					<align hpos="-1"><label text="labels.status" /></align>
					<align hpos="-1"><label text="labels.threads" /></align>
					<align hpos="-1"><label text="labels.quality" /></align>
					<align hpos="-1"><label text="labels.progress" /></align>
	
					<align hpos="-1" expand="true"><status id="status" /></align>
					<align hpos="-1">
						<hbox spacing="4">
							<threadcombo id="threads" width="32" />
							<button id="normal" led="true" color="yellow" size="16" />
							<label text="labels.normalize" />
						</hbox>
					</align>
					<align hpos="-1">
						<hbox spacing="4">
							<fader id="quality" angle="0" size="64" />
							<value id="quality" sline="true" />
						</hbox>
					</align>
					<cell cols="2"><progress id="prog" /></cell>
					
					<cell rows="3">
						<vbox>
							<label text="labels.launch" visibility=":status ine 3" />
							<button id="render" led="true" color="green" size="32" visibility=":status ine 3" />
							<label text="labels.stop" visibility=":status eq 3" />
							<button id="render" led="true" color="red" size="32" visibility=":status ieq 3" />
						</vbox>
					</cell>
				</grid>
			</group>
			
			<!-- Output -->
			<group text="groups.output" expand="false">
				<hbox>
					<grid rows="4" cols="3" expand="true" fill="true">
						<cell cols="2"><label text="labels.signal.dry_pan" /></cell>
						<cell rows="2"><label text="labels.predelay" /></cell>
						<label text="labels.chan.L_eft" />
						<label text="labels.chan.R_ight" />
						
						<knob id="pl" scolor="left_channel" size="16" />
						<knob id="pr" scolor="right_channel" size="16" />
						<knob id="pd" />
						
						<value id="pl" />
						<value id="pr" />
						<value id="pd" />
					</grid>
					<vsep />
					
					<grid hspacing="4" rows="3" cols="3" expand="true" fill="true">	
						<label text="labels.signal.dry" />
						<label text="labels.signal.wet" />
						<label text="labels.output" />
						
						<knob id="dry" scolor="yellow" />
						<knob id="wet" scolor="yellow" />
						<knob id="g_out" />
						
						<value id="dry" />
						<value id="wet" />
						<value id="g_out" />
					</grid>
				</hbox>
			</group>
		</hbox>
			
		<!-- IR routing -->
		<cgroup id="signal">
			<!-- Processors -->
			<hbox>
				<grid rows="5" cols="6" expand="true">
					<label text="labels.specials.num" />
					<label text="labels.signal.in_lr" />
					<label text="labels.predelay" />
					<label text="labels.source" />
					<label text="labels.makeup" />
					<label text="labels.signal.out_lr" />
					
					<ui:for id="i" first="0" last="1">
						<label text="${i}" />
						<knob id="cim${i}" size="20" />
						<knob id="pd${i}" size="20" />
						<vbox spacing="2">
							<combo id="csf${i}" fill="true" />
							<combo id="cst${i}" fill="true" />
						</vbox>
						<knob id="mk${i}" size="20" />
						<knob id="com${i}" size="20" />
						
						<label />
						<value id="cim${i}" sline="true" />
						<value id="pd${i}" sline="true" />
						<hbox fill="false">
							<button id="cam${i}" size="12" color="red" led="true" />
							<label text="labels.chan.mute" font.size="9" />
							<led id="ca${i}" />
							<label text="labels.active" font.size="9" />
						</hbox>
						<value id="mk${i}" sline="true" />
						<value id="com${i}" sline="true" />
					</ui:for>
				</grid>
				<vsep />
				<grid rows="5" cols="6" expand="true">
					<label text="labels.specials.num" />
					<label text="labels.signal.in_lr" />
					<label text="labels.predelay" />
					<label text="labels.source" />
					<label text="labels.makeup" />
					<label text="labels.signal.out_lr" />
					
					<ui:for id="i" first="2" last="3">
						<label text="${i}" />
						<knob id="cim${i}" size="20" />
						<knob id="pd${i}" size="20" />
						<vbox spacing="2">
							<combo id="csf${i}" fill="true" />
							<combo id="cst${i}" fill="true" />
						</vbox>
						<knob id="mk${i}" size="20" />
						<knob id="com${i}" size="20" />
						
						<label />
						<value id="cim${i}" sline="true" />
						<value id="pd${i}" sline="true" />
						<hbox fill="false">
							<button id="cam${i}" size="12" color="red" led="true" />
							<label text="labels.chan.mute" font.size="9" />
							<led id="ca${i}" />
							<label text="labels.active" font.size="9" />
						</hbox>
						<value id="mk${i}" sline="true" />
						<value id="com${i}" sline="true" />
					</ui:for>
				</grid>
			</hbox>
			
			<!-- Equalizer -->
			<grid rows="5" cols="10">
				<ui:with bright="(:wpp) ? 1 : 0.5">
					<label text="labels.flt.low_cut" />
					<label text="50" />
					<label text="107" />
					<label text="227" />
					<label text="484" />
					<label text="labels.flt.1k" />
					<label text="labels.flt.2_2k" />
					<label text="labels.flt.4_7k" />
					<label text="labels.flt.10k" />
					<label text="labels.flt.high_cut" />
					
					<combo id="lcm" />
					<ui:for id="f" first="0" last="7"> 
						<cell rows="3"><fader id="eq_${f}" angle="1" bright="(:wpp) ? 1 : 0.5" /></cell>
					</ui:for>
					<combo id="hcm" />
					
					<label text="labels.frequency" />
					<label text="labels.frequency" />
					
					<knob id="lcf" />
					<knob id="hcf" />
					
					<value id="lcf" />
					<ui:for id="f" first="0" last="7">
						<value id="eq_${f}" bright="(:wpp) ? 1 : 0.5" />
					</ui:for>
					<value id="hcf" />
				</ui:with>
			</grid>
		</cgroup>
	</vbox>
</plugin>